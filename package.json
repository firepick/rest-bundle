{
  "name": "rest-bundle",
  "version": "0.9.23",
  "description": "RestBundle is a Javascript base class for a REST resource bundles that can be plugged into a nodejs express application",
  "main": "index.js",
  "directories": {
    "test": "test",
    "dist": "dist",
    "src": "src",
    "doc": "doc"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "file-loader": "^0.11.1",
    "vue": "^2.3.2",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "concurrently": "^3.2.0",
    "css-loader": "^0.28.0",
    "mocha": "^3.2.0",
    "should": "^11.2.0",
    "supertest": "^3.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.2.0"
  },
  "scripts": {
    "build": "bash scripts/build",
    "build:aot": "ngc -p src/ui/tsconfig-aot.json && rollup -c cfg/rollup-config.js",
    "build:dist": "npm run clean && npm run build:aot && webpack --config cfg/webpack.config.js",
    "build:e2e": "tsc -p e2e/",
    "build:jit": "tsc -p src/ui/tsconfig-jit.json",
    "build:lib": "tsc -p tsconfig-lib.json",
    "build:pack": "webpack --config cfg/webpack.config.js",
    "build:vue": "cross-env NODE_ENV=production webpack --config cfg/webpack.vue.js --progress --hide-modules",
    "build:watch": "tsc -p src/ui/tsconfig-aot.json -w",
    "clean": "rm -f *.log*; rimraf dist && rimraf src/ui/aot && rimraf lib",
    "dev": "concurrently \"scripts/demo-node.js\" \"npm run dev:vue\"",
    "dev:vue": "cross-env NODE_ENV=development webpack-dev-server --config cfg/webpack.vue.js --open --hot",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "pree2e": "npm run build:e2e",
    "preprotractor": "webdriver-manager update",
    "TBD-prestart": "npm run build",
    "TBD-pretest": "npm run build",
    "pretest:once": "npm run build",
    "protractor": "protractor protractor.config.js",
    "serve": "lite-server -c=cfg/bs-config.json",
    "serve:e2e": "lite-server -c=cfg/bs-config.e2e.json",
    "start": "node scripts/hello-node.js ",
    "start:ts": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "test": "npm run test:node",
    "test:node": "mocha -w -R min --inline-diffs",
    "test:detail": "mocha -w --inline-diffs",
    "test:once": "karma start karma.conf.js --single-run",
    "z": "echo z"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firepick/rest-bundle.git"
  },
  "bin": {},
  "keywords": [
    "REST",
    "resource bundle",
    "express",
    "FirePick",
    "firenodejs"
  ],
  "author": "Karl Lew",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firepick/rest-bundle/issues"
  },
  "homepage": "https://github.com/firepick/rest-bundle"
}
